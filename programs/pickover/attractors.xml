<attractors>
	<attractor name="pickover">
		<code>
			[nx] = sin([a] * [y] +[f]) - [z] * cos( [b] * [x] + [g] );
			[ny] = [z] * sin([c] * [x] + [h]) - cos( [d] * [y] + [i] );
			[nz] = [e] * sin([n] * [x] + [j]);
		</code>
		<parameters>
			<parameter name="a" min="-3" max="3" />
			<parameter name="b" min=".5" max="3" />
			<parameter name="c" min="-3" max="3" />
			<parameter name="d" min=".5" max="3" />
			<parameter name="e" min=".5" max="3" />
			<parameter name="f" min="-3" max="3" />
			<parameter name="g" min="-3" max="3" />
			<parameter name="h" min="-3" max="3" />
			<parameter name="i" min="-3" max="3" />
			<parameter name="j" min="-3" max="3" />
			<parameter name="n" min="1" max="3" />
		</parameters>
	</attractor>
	<attractor name="unravel">
		<code>
			[nx] = [z] - [a] * [y] * [y];
			[ny] = [x] - [b] * [z] * [z];
			[nz] = [y] - [c] * [x] * [x]; 
		</code>
		<parameters>
		<parameter name="a" min="1" max="5" />
		<parameter name="b" min="-5" max="5" />
		<parameter name="c" min="-5" max="5" />
		</parameters>
	</attractor>
</attractors>
